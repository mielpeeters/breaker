
tempo 120 4/4

grid beat {
	1_2_3_2_
	1_2_3_2_
} map beat {
	1: kick,
	2: hihat8,
	3: snare,
} note beat 1/16

grid andromeda {
	[2]d_[2]d_
	[2]d [2]e [2]c 
	[2]d&_[2]d_
	[2]d [2]e [2]c [2]d

	[1]a_[1]a_
	[1]a [1]t [1]g 
	[1]a&_[1]a_
	[1]a [1]t [2]c [1]a
} note andromeda 1/8

set andromeda gain = 0.08
set andromeda lp_cutoff = 1200.0
mix andromeda 2.0
set beat gain = 1.1
mix beat 1.0
set beat lp_cutoff = 5000.0

grid andro {
	[3]Dm _
	[3]Dm _
	[3]Dm _
	[3]Dm _
	[2]Am _
	[2]Am _
	[2]Am _
	[2]Am _
} note andro 1/4

set andro gain = 0.1
set andro lp_cutoff = 1200.0
mix andro 0.2
